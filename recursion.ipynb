function powerSet(string) {
  var result = [""];
  
  var helper = function(index) {
    if(index >= string.length) {
      return result;
    } else {
      var copyResult = result.slice(0);  // index : 0 ,  result = [""]  "a"
      for(var i = 0; i < copyResult.length; i++) {   // i = 0
        var temp = copyResult[i] + string[index];  //  "" + "a"
        result.push(temp);
      }
      helper(index + 1);
    }
  }
  helper(0);
  return result;
