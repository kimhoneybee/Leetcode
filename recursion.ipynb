    "def powerset(string):\n",
    "   result = [\"\"]\n",
    "   def helper(index):\n",
    "     copyResult = result[:]\n",
    "     if index >= len(string):\n",
    "       return result\n",
    "     for i in range(0,len(copyResult)):  # [\"\", \"a\"]\n",
    "       temp = copyResult[i] + string[index] # \"\" + string[1](\"b\") => \"b\", \"a\"+ \"b\" => \"ab\"\n",
    "       result.append(temp)\n",
    "     helper(index+1)\n",
    "   helper(1)\n",
    "   print(result)\n",
    "   return result;\n",
    "    \n",
    " powerset(\"abc\")\n"
